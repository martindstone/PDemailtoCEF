<!DOCTYPE html>
<html>
  <head>
    <title>PDemailtoCEF</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="/">PagerDuty Email to Events v2 Gateway</a>
    </nav>
    <div class="container-fluid" id="application-content">
        <%= flash_messages %>
        <%= yield %>
    </div>
  </body>
</html>
